               const_cast<thisType&>(*this).bfMax = source.bfMax;
            else if (compareThisMaxSourceMax == CRGreater)
               const_cast<thisType&>(source).bfMax = bfMax;
         };
         inherited::notifyForBranchCompare(*this);
         inherited::notifyForBranchCompare(source);
         result = doesTakeThen;
      }
   }
   return result;
}

